{
  "dependencies": {
    "bulma": "^0.9.3",
    "fuse.js": "^6.5.3"
  },
  "name": "julia-tldr-website",
  "version": "1.0.0",
  "description": "Site for TLDR.jl",
  "scripts": {
    "css-build": "node-sass --omit-source-map-url sass/style.scss css/style.css",
    "css-watch": "npm run css-build -- --watch",
